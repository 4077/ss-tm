<tr class="record {CLASS}" record_id="{ID}">

    <td class="client_cell" width="1">
        <div class="main">
            <div class="avatar">
                <img src="{AVATAR_SRC}">
            </div>
            <div class="client {CLIENT_CLASS}">
                <div class="row">
                    <div class="name">{CLIENT_NAME}</div>
                    <div class="human {IS_HUMAN_CLASS}">
                        <div class="icon fa fa-male"></div>
                    </div>
                    <div class="device">
                        <div class="icon {DEVICE_ICON_CLASS}"></div>
                    </div>
                    <div class="city">
                        {CLIENT_CITY}
                    </div>
                </div>
                <div class="row">
                    <div class="datetime">{DATETIME}</div>
                </div>
            </div>
        </div>
    </td>

    <td class="event_cell" width="1">
        <div class="event">
            <div class="icon {EVENT_ICON_CLASS}"></div>
            <div class="name">
                {EVENT_NAME}
            </div>
        </div>
    </td>

    <td>
        <div class="event_desc">
            {EVENT_DESC}

            <div class="human_actions_bar">

            </div>
        </div>
    </td>

</tr>
